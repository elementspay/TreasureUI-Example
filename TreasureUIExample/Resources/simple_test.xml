<?xml version="1.0" encoding="UTF-8"?>
<treasure_view>
  <context>
  <id>global_context</id>
  <value>
    <items>
      <id>payment_method_item_apple_pay_1</id>
      <type>blueprint</type>
      <blueprint_id>payment_method_item</blueprint_id>
      <context>
        <id>context</id>
        <value>
          <leading_image>
            <type>local_image</type>
            <value>checkmark_grey</value>
          </leading_image>
          <secondary_image>
            <type>local_image</type>
            <value>checkmark_green</value>
          </secondary_image>
          <item_image>
            <type>local_image</type>
            <value>apple_pay_icon</value>
          </item_image>
          <title>Apple Pay</title>
          <trailing_image>
            <type>local_image</type>
            <value>disclosure_indicator_dark</value>
          </trailing_image>
          <trailing_image_style_id>image_extra_small</trailing_image_style_id>
          <on_press_action>
            <type>push_view</type>
            <route>
              <screen_id>push_view_id</screen_id>
            </route>
          </on_press_action>
        </value>
      </context>
    </items>
    <items>
      <id>payment_method_item_card_1</id>
      <type>blueprint</type>
      <blueprint_id>payment_method_item</blueprint_id>
      <context>
        <id>context</id>
        <value>
          <leading_image>
            <type>local_image</type>
            <value>checkmark_grey</value>
          </leading_image>
          <item_image>
            <type>local_image</type>
            <value>payment_mastercard</value>
          </item_image>
          <title>•••• 9023 (Default)</title>
          <trailing_image>
            <type>local_image</type>
            <value>disclosure_indicator_dark</value>
          </trailing_image>
          <trailing_image_style_id>image_extra_small</trailing_image_style_id>
        </value>
      </context>
    </items>
    <items>
      <id>payment_method_item_card_2</id>
      <type>blueprint</type>
      <blueprint_id>payment_method_item</blueprint_id>
      <context>
        <id>context</id>
        <value>
          <leading_image>
            <type>local_image</type>
            <value>checkmark_grey</value>
          </leading_image>
          <item_image>
            <type>local_image</type>
            <value>ic_rider_payment_paypal</value>
          </item_image>
          <title>Paypal</title>
          <subtitle>marvin.zhan@gmail.com</subtitle>
          <trailing_image>
            <type>local_image</type>
            <value>delete_card_gray</value>
          </trailing_image>
          <trailing_image_style_id>image_delete_icon</trailing_image_style_id>
          <on_press_action>
            <type>push_view</type>
            <route>
              <screen_id>push_view_id</screen_id>
            </route>
          </on_press_action>
        </value>
      </context>
    </items>
  </value>
  </context>
  <screens>
  	<id>root</id>
  	<name>root</name>
  	<screen_id>root</screen_id>
  	<style>root_screen_style</style>
  	<layout>
  		<compact>
  			<type>single_column</type>
  			<main>
  				<section_details section_id="payment_methods_display_lime"/>
  			</main>
  			<nav>
  				<section_details section_id="nav_bar_mobile"/>
  			</nav>
  			<floating_footer>
  			</floating_footer>
  		</compact>
  	</layout>
  </screens>
  <sections>
  	<id>nav_bar_mobile</id>
  	<type>general_list_content</type>
    <section>
      <items>
        <id>nav_bar</id>
        <type>nav_bar</type>
        <style>nav_bar_style</style>
        <title>Payments</title>
        <image>
          <type>local_image</type>
          <value>supreme_close_light_background</value>
        </image>
      </items>
    </section>
  </sections>
  <sections>
    <id>payment_methods_display_lime</id>
    <type>general_list_content</type>
    <section>
      <title>Test if this works</title>
      <items>
        <id>label_title</id>
        <type>label</type>
        <style>header_text</style>
        <title>PAYMENT METHODS</title>
      </items>
      <items>
        <id>divider_apple_pay</id>
        <type>divider</type>
        <style>horizontal_divider</style>
      </items>
      <items>
        <id>items_placeholder</id>
        <type>context_element</type>
        <items>@{global_context.items}</items>
      </items>
      <items>
        <id>divider_7</id>
        <type>divider</type>
        <style>horizontal_divider</style>
      </items>
      <items>
        <id>divider_3</id>
        <type>divider</type>
        <style>spacer_divider</style>
      </items>
      <items>
        <id>divider_6</id>
        <type>divider</type>
        <style>horizontal_divider</style>
      </items>
      <items>
        <id>item_12</id>
        <type>basic_list_item</type>
        <style>list_secondary_add_card</style>
        <title>Add card</title>
        <image>
          <type>local_image</type>
          <value>ic_supreme_payment_creditcard</value>
        </image>
        <on_press_action>
          <type>external_action</type>
          <action_type>show_add_card_view</action_type>
        </on_press_action>
      </items>
      <items>
        <id>divider_5</id>
        <type>divider</type>
        <style>horizontal_divider</style>
      </items>
    </section>
  </sections>
  <blueprints>
    <id>payment_method_item</id>
    <type>h_group</type>
    <style>h_group_secondary</style>
    <items>
      <id>image_2</id>
      <type>image</type>
      <style>image_small</style>
      <accessibility_label>3</accessibility_label>
      <image>@{context.leading_image}</image>
    </items>
    <items>
      <id>image_21</id>
      <type>image</type>
      <style>image_small</style>
      <accessibility_label>3</accessibility_label>
      <image>@{context.secondary_image}</image>
    </items>
    <items>
      <id>image_3</id>
      <type>image</type>
      <style>image_secondary</style>
      <image>@{context.item_image}</image>
    </items>
    <items>
      <id>item_11</id>
      <type>basic_list_item</type>
      <style>list_secondary</style>
      <title>@{context.title}</title>
      <subtitle>@{context.subtitle}</subtitle>
    </items>
    <items>
      <id>image_4</id>
      <type>image</type>
      <style>@{context.trailing_image_style_id}</style>
      <accessibility_label>3</accessibility_label>
      <image>@{context.trailing_image}</image>
      <on_press_action>@{context.on_press_action}</on_press_action>
    </items>
    <on_press_action>
      <type>present_view</type>
      <route>
        <file_name>treasure_view</file_name>
      </route>
    </on_press_action>
  </blueprints>
  <styles>
    <type>screen</type>
    <id>root_screen_style</id>
    <basic_props>
      <background_color>#F2F2F2</background_color>
    </basic_props>
  </styles>
  <styles>
    <type>nav_bar_style</type>
    <id>nav_bar_style</id>
    <bottom_line_height>1</bottom_line_height>
    <bottom_line_visibility>visible_by_offset</bottom_line_visibility>
    <bottom_line_color>#CCCCCC</bottom_line_color>
    <title_style_id>primary_text</title_style_id>
    <image_style_id>image_nav_bar</image_style_id>
    <height>80</height>
    <basic_props>
      <background_color>#F2F2F2</background_color>
      <layout_margins>24</layout_margins>
      <layout_margins>12</layout_margins>
      <layout_margins>12</layout_margins>
      <layout_margins>12</layout_margins>
    </basic_props>
  </styles>
  <styles>
    <type>image</type>
    <id>image_nav_bar</id>
    <image_height>70</image_height>
    <image_width>70</image_width>
  </styles>
  <styles>
    <type>label</type>
    <id>header_text</id>
    <alignment>leading</alignment>
    <number_of_lines>1</number_of_lines>
    <text_color>#000000</text_color>
    <text_font>
      <name>Poppins-Medium</name>
      <size>14</size>
    </text_font>
    <basic_props>
      <layout_margins>12</layout_margins>
      <layout_margins>24</layout_margins>
      <layout_margins>12</layout_margins>
      <layout_margins>12</layout_margins>
    </basic_props>
  </styles>
  <styles>
    <type>label</type>
    <id>primary_text</id>
    <alignment>center</alignment>
    <number_of_lines>1</number_of_lines>
    <text_color>#000000</text_color>
    <text_font>
      <name>Poppins-Medium</name>
      <size>18</size>
    </text_font>
  </styles>
  <styles>
    <type>label</type>
    <id>list_item_title</id>
    <alignment>leading</alignment>
    <number_of_lines>1</number_of_lines>
    <text_color>#000000</text_color>
    <text_font>
      <name>Avenir-Medium</name>
      <size>17</size>
    </text_font>
  </styles>
  <styles>
    <type>label</type>
    <id>secondary_text</id>
    <alignment>leading</alignment>
    <number_of_lines>1</number_of_lines>
    <text_color>#878883</text_color>
    <text_font>
      <name>Avenir</name>
      <size>16</size>
    </text_font>
  </styles>
  <styles>
    <type>label</type>
    <id>green_text</id>
    <alignment>leading</alignment>
    <number_of_lines>1</number_of_lines>
    <text_color>#00C700</text_color>
    <text_font>
      <name>Avenir</name>
      <size>18</size>
    </text_font>
  </styles>
  <styles>
    <type>image</type>
    <id>image_primary</id>
    <image_height>100</image_height>
    <image_width>100</image_width>
    <corner_radius>4</corner_radius>
  </styles>
  <styles>
    <type>image</type>
    <id>image_secondary</id>
    <image_height>40</image_height>
    <image_width>40</image_width>
    <corner_radius>4</corner_radius>
  </styles>
  <styles>
    <type>image</type>
    <id>image_small</id>
    <image_height>24</image_height>
    <image_width>24</image_width>
    <corner_radius>4</corner_radius>
    <basic_props>
      <layout_margins>0</layout_margins>
      <layout_margins>0</layout_margins>
      <layout_margins>0</layout_margins>
      <layout_margins>16</layout_margins>
    </basic_props>
  </styles>
  <styles>
    <type>image</type>
    <id>image_extra_small</id>
    <image_height>16</image_height>
    <image_width>8</image_width>
    <basic_props>
      <layout_margins>0</layout_margins>
      <layout_margins>0</layout_margins>
      <layout_margins>0</layout_margins>
      <layout_margins>16</layout_margins>
    </basic_props>
  </styles>
  <styles>
    <type>image</type>
    <id>image_delete_icon</id>
    <image_height>52.5</image_height>
    <image_width>30</image_width>
    <basic_props>
      <layout_margins>0</layout_margins>
      <layout_margins>0</layout_margins>
      <layout_margins>0</layout_margins>
      <layout_margins>16</layout_margins>
    </basic_props>
  </styles>
  <styles>
    <id>list_secondary</id>
    <type>basic_list_item</type>
    <h_group_alignment>fill</h_group_alignment>
    <h_spacing>12</h_spacing>
    <title_subtitle_style_id>title_subtitle_list_item</title_subtitle_style_id>
    <image_style_id>image_secondary</image_style_id>
    <basic_props>
      <background_color>#ffffff</background_color>
      <layout_margins>12</layout_margins>
      <layout_margins>8</layout_margins>
      <layout_margins>12</layout_margins>
      <layout_margins>16</layout_margins>
    </basic_props>
  </styles>
  <styles>
    <id>list_secondary_add_card</id>
    <type>basic_list_item</type>
    <alignment>leading</alignment>
    <h_spacing>16</h_spacing>
    <title_subtitle_style_id>title_subtitle_list_item_green</title_subtitle_style_id>
    <image_style_id>image_secondary</image_style_id>
    <basic_props>
      <layout_margins>12</layout_margins>
      <layout_margins>16</layout_margins>
      <layout_margins>12</layout_margins>
      <layout_margins>16</layout_margins>
      <background_color>#ffffff</background_color>
    </basic_props>
  </styles>
  <styles>
    <id>title_subtitle_list_item_green</id>
    <type>title_and_subtitle</type>
    <alignment>leading</alignment>
    <vertical_spacing>0</vertical_spacing>
    <title_style_id>green_text</title_style_id>
    <subtitle_style_id>secondary_text</subtitle_style_id>
  </styles>
  <styles>
    <id>title_subtitle_list_item</id>
    <type>title_and_subtitle</type>
    <alignment>leading</alignment>
    <vertical_spacing>0</vertical_spacing>
    <title_style_id>list_item_title</title_style_id>
    <subtitle_style_id>secondary_text</subtitle_style_id>
  </styles>
  <styles>
    <id>h_group_primary</id>
    <type>group</type>
    <h_group_align>fill</h_group_align>
    <spacing>8</spacing>
    <basic_props>
      <layout_margins>0</layout_margins>
      <layout_margins>24</layout_margins>
      <layout_margins>0</layout_margins>
      <layout_margins>0</layout_margins>
      <background_color>#ffffff</background_color>
    </basic_props>
  </styles>
  <styles>
    <id>h_group_secondary</id>
    <type>group</type>
    <h_group_align>fill</h_group_align>
    <spacing>8</spacing>
    <basic_props>
      <layout_margins>8</layout_margins>
      <layout_margins>24</layout_margins>
      <layout_margins>8</layout_margins>
      <layout_margins>0</layout_margins>
      <background_color>#ffffff</background_color>
    </basic_props>
  </styles>
  <styles>
    <id>h_group_leading</id>
    <type>group</type>
    <h_group_align>top</h_group_align>
    <spacing>4</spacing>
  </styles>
  <styles>
    <id>horizontal_divider</id>
    <type>divider</type>
    <height>1</height>
    <color>#CCCCCC</color>
  </styles>
  <styles>
    <id>spacer_divider</id>
    <type>divider</type>
    <height>10</height>
    <color>#F2F2F2</color>
  </styles>
</treasure_view>

